}
/*------------image links in the left nav homepgae---------------------*/
.imageLink {
    position:relative;
    background:#F05449;
    margin-bottom:10%;
    overflow:hidden;
}
.imageLink:hover {
    opacity:0.7;
}
.imageLink.even {
    background:#3265B0;
}
.imageLinkText {
    width:80%;
    margin-left:10%;
    position:absolute;
    top:32%;
    text-align:center;
}

