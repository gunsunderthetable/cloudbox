    clear: both; 
}

/*--------------------------------responsivity-----------------------------------*/
    
        @media only screen and (max-width: 1270px) {
            .mainNav {font-size:1.2em;}
        }
        @media only screen and (max-width: 1130px) {
            .mainNav {font-size:1.1em;}
            .mainNav a {padding:0 10px;}
        }     
        /*-----970px----------*/
        @media only screen and (max-width: 960px) {
            img.mobileMast {width:26%;padding-left:80px;height:auto;display:block}
            .mobileOnly {display:block;width:100%;height:auto;}
            #mobileNav {display:block;z-index:999;}
            .desktopTabletOnly {display:none;}
            
            .three.columns.sidePanel {width:47.25%;margin-right:5%;}
            .nine.columns.mainPanel {width:47.25%;}
             
            form {margin:0;}
            #mastheadMobile {z-index:998;background:white;}
            #homePageLinksBox {padding:3%;}
            #homePageLinksBox ul li {background-image:none;padding-left:0;}
            .flexslider {display:none;}
            #contactDetails,#footerLinks {width:100%;text-align:center;}
            #partners {display:none;}
            #subFooter p.lefty, #subFooter p.righty {width:100%;clear:both;text-align:center;} 
            #subNav {padding:40px;}
            
            #SearchForm_SearchForm {top:25%;right:5px;width:45%;}
            #SearchForm_SearchForm input[type="text"] {width:200px}
            
        }

        @media only screen and (max-width: 570px) {
            .three.columns.sidePanel {width:100%;margin-right:0%;}
            .nine.columns.mainPanel {width:100%;}
            .imageLink {display:none;}
            #SearchForm_SearchForm {top:10%;right:5px;width:52%;}
        }
        
