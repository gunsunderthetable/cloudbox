/*---------------------------------------- homepage boxes ---------------------------------------------------*/

.box{
    position: relative;
    width: 23.5%;	
    margin:0 .75% 1.5%;
    color:silver;
    float:left;    
    background:#EAE9E6;
    overflow:hidden;
    -webkit-transition: all .3s ease; -moz-transition: all .3s ease; transition: all .3s ease;
}

.box:before{
    content: "";
    display: block;
    padding-top: 100%; 
}

.content{
    /* this is within the box--*/
    position:  absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
}


.treeview ul li span{
    height:55px;
}



.box:hover {cursor:pointer;}

.box:hover {background-color:#F4F3F0;}

