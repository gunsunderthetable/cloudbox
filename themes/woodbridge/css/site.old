    line-height:1;
    letter-spacing:1px;
    color:white;
    font-weight:700;
    font-family: 'Yanone Kaffeesatz', sans-serif;
}
.imageLink h2 {
    color:white;
    font-size:1.8em;
    line-height:1;
    margin-bottom:8px;
    
}

.imageLink img {
    width:100%;
    height:auto;
    opacity:0.8;
    display:block;
}
.imageLink a {line-height:-1;}
/*------------------standard page stuff-----------------------*/
#contentPanel {
    padding:40px;
    background:white;
    border:1px solid #A7997A;
    margin-bottom:40px;
}

/*--------------------the message flash bar ------------------------*/
