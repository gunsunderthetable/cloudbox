}
/* --------------subNavigation --------------------------*/

#subNav {
    padding:20px;
    background:white;
    border:1px solid #A7997A;
    margin-bottom:40px;    
}

#subNav h3, .widget h3 {
    font-size:1.8em;
}
#subNav ul {
    list-style-type:none;
    margin-left:0;

}
#subNav li a {
/*    text-transform:uppercase;*/
}

