body {
    background:#FBF1DF;
}


/*--------------------mobile odds-------------------------*/
.mobileOnly {
    display:none;
}
#sidr {display:none;}
#mastheadMobile {
    position:relative;
}
#mobileNav {
    position:absolute;
    top:35%;
    left:3%;
    display:none;
}

/*--------------------------------------------------------*/

a {
    color:#1b99e2
}
a:hover {color:black}

#backgroundWrap {
  background:#DEE8F6;
  padding:40px 3% 0;
  width: 100%;
  max-width: 1280px;
  box-sizing: border-box; 
  margin: 0 auto;
}
.container {
  position: relative;
  width: 100%;
  padding: 0;
}

input[type="submit"] {background:#1b99e2;color:white;border:0;border-radius:9px;}

#breadBox {
    margin-bottom:10px;
}
/*-------------------------------------------------------------------------*/
#shadowWrap {
    box-shadow:6px 0 4px rgba(0, 0, 0, 0.2),-6px 0 4px rgba(0, 0, 0, 0.2);
}

#header, #shadowWrap {
    position:relative;
    overflow:hidden;
    background:white;
    width: 100%;
    max-width: 1280px;  
    margin: 0 auto;
}

#header #mainBar {
    position:relative;
    overflow:hidden;
    /*height:180px;*/
}
#header .branding {
    background:white;
    width:21.5%;
    float:left;
    position:relative;
    padding-top:11px;
    
}
#header .branding img {
    width:95%;
    padding:3% 0 0 5%;
    height:auto
}
#header .mastImage {
    float:right;
    width:76.5%;
}
.mastImage img {
    display:block;
    width:100%;
    height:auto;
}

#SearchForm_SearchForm {
    position:absolute;
    bottom:16px;
    right:-5px;
    width:320px;
}
#SearchForm_SearchForm input[type="text"] {
    margin:3px 0 0 0;
    width:260px;
    float:left;
}
#SearchForm_SearchForm input[type="submit"] {
    background: white url(../images/magnifying.png) no-repeat top left;
    border:1px solid white;
    color:transparent;
    font-weight:700;
    font-size:1.1em;
    margin:4px 0 0 0;
    width:35px;
    padding:0;
    height:36px;
}
#searchResults p span.highlight {
    padding:0 4px;
    background:#eef;
}

.mobileOnly {
    display:none;
}
/* main nav -----------------------------------*/


.mainNav {
    font-family: 'Yanone Kaffeesatz', sans-serif;
    font-size:1.8em;
    width:100%;
    background:#3265B0;
    height:40px;
    padding-top:4px;
/*    text-align:center;*/
    overflow:hidden;

}
.mainNav ul {
    float:right;
    padding:0;
    margin:0;
}

.mainNav li{
    float:left;
    margin:0;
}

.mainNav a{
	display:block;
	float:left;
	padding:0 14px;
	height:34px;
	line-height:34px;
    font-weight:400;

        color:white;
}

.mainNav a.current,
.mainNav a.section,
.mainNav a:hover,
.mainNav a:focus {
	color:#1b99e2;
}

/*-------------------------homepage left hand nav-----------------------------------*/

#homePageLinksBox {
    background:#3265B0;
    padding:8%;
    margin-bottom:8%;
    
}
#homePageLinksBox h2 {
    font-family: 'Yanone Kaffeesatz', sans-serif;

    color:white;
    font-size:3em;
    font-weight:400;
}
#homePageLinksBox li a {
    color:white;
    font-size:1.2em;
}
#homePageLinksBox li a:hover {
