/* Pagination
 * ------------------------------------- */

.pagination{
	padding:12px 0 0;
        clear:left;
        font-weight:bold;
        text-align:center;
}


ul.pagination {
    float:right;
}

.pagination li{
        list-style-type:none;
	float:left;
	line-height:1;
}

.pagination a, .pagination .noLink{
	float:left;
	display:block;
	border-right:1px solid #c1c1c1;
	padding:0 8px;
}

.pagination li.last,
.pagination .last a
{
	border-right:none;
}

.pagination .prev a,
.pagination .next a{
	border:none;
	padding:0;
}

.pagination .prev a{
	padding-right:16px;
}

.pagination .next a{
	padding-left:16px;
}


/*--------------------------------------IE stuff-------------------------------------------*/


.ie .homePageRow {overflow:hidden;}
.ie .homePageRow h1 {font-family:arial, san-serif;font-weight:900;font-size:24px;}
.ie .homePageRow.Short {height:200px;}
